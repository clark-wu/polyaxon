{
  "name": "polyaxon-ui",
  "version": "0.4.1",
  "description": "Polyaxon ui",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/* && rm -rf public/dist/* && mkdir -p public/dist/js && mkdir public/dist/css && mkdir public/dist/fonts",
    "_copy_react": "cp node_modules/react/umd/react.production.min.js node_modules/react-dom/umd/react-dom.production.min.js public/dist/js",
    "_copy_css": "cp node_modules/font-awesome/css/font-awesome.min.css node_modules/bootstrap/dist/css/bootstrap.min.css public/dist/css",
    "_copy_fonts": "cp -r node_modules/font-awesome/fonts public/dist/",
    "copy": "npm run _copy_react && npm run _copy_css && npm run _copy_fonts",
    "uglify_css": "uglifycss public/css/global.css --output public/dist/css/global.min.css && uglifycss public/css/global_modal.css --output public/dist/css/global_modal.min.css && uglifycss public/css/loader.css --output public/dist/css/loader.min.css",
    "uglify_js": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output public/dist/js/bundle.min.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --port 8001 --env.dev --config webpack.config.dev.js  --output-public-path=dist/",
    "build": "npm run clean && webpack --env.prod --config webpack.config.prod.js && npm run uglify_js && npm run uglify_css && npm run copy"
  },
  "author": "mourad mourafiq",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/history": "4.7.2",
    "@types/js-cookie": "2.1.0",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "4.14.121",
    "@types/node": "8.10.20",
    "@types/plotly.js": "^1.38.8",
    "@types/query-string": "6.1.0",
    "@types/raven": "2.5.1",
    "@types/react": "16.8.5",
    "@types/react-bootstrap": "0.32.17",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "6.0.2",
    "@types/react-router": "4.4.4",
    "@types/react-router-bootstrap": "0.24.5",
    "@types/react-router-dom": "4.3.1",
    "@types/react-select": "^2.0.13",
    "@types/redux-logger": "3.0.6",
    "@types/sanitize-html": "^1.18.2",
    "@types/showdown": "^1.7.5",
    "@types/sprintf-js": "0.0.27",
    "awesome-typescript-loader": "3.5.0",
    "bootstrap": "3.4.1",
    "css-loader": "0.28.11",
    "file-loader": "0.11.2",
    "font-awesome": "4.7.0",
    "fontawesome": "4.7.2",
    "less": "2.7.3",
    "less-loader": "4.1.0",
    "redux-logger": "3.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.18.2",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "uglify-es": "3.3.9",
    "uglifycss": "0.0.29",
    "url-loader": "0.5.9",
    "webpack": "3.12.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.0.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.8",
    "history": "4.7.2",
