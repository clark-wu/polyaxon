    "js-yaml": "^3.12.0",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "normalizr": "3.2.4",
    "plotly.js": "1.45.0",
    "query-string": "6.2.0",
    "raven-js": "3.26.3",
    "react": "16.8.3",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.8.3",
    "react-mde": "^7.0.3",
    "react-redux": "6.0.1",
    "react-router": "4.3.1",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.3.1",
    "react-select": "^2.4.1",
    "react-treebeard": "2.1.0",
    "redux": "4.0.1",
